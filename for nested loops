#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    
    int T, a, b, N, j, i;
    cin >> T;
    
    for (i = 0; i < T; i++)
        {              
                    cin >> a;
                    cin >> b;
                    cin >> N;
        
            for (j = 0; j <= N - 1; j++) 
                {
                    a += (pow (2,j) *b);
                    cout << a << " ";
                 }
            cout << endl;
    }
    return 0;
}

