#include <cstdio>
#include <cstdlib>

using namespace std;

int main(){
    int n, i;
    cin >> n;
    int tab[n];
    
    for (i = 0; i < n; i++)
        {
           cin >> tab[i];
        }
    
    for (int i = n - 1; i >= 0; i--) {
            cout << tab[i] << " ";
        }
    
    return 0;
}
